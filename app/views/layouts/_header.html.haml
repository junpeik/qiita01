%header.navbar.navbar-default
  .container
    .navbar-nav
      = link_to 'Qiita', root_path
    .navbar-nav.pull-right
      - if user_signed_in?
        = link_to '投稿する', '#'
        = link_to 'ストック一覧', '#'
        = link_to 'マイページ', '#'
        = link_to '設定', '#'
        = link_to 'ログアウト', destroy_user_session_path, method: :delete
      - else
        = link_to 'ユーザ登録', new_user_registration_path
        = link_to 'ログイン', '#'
  - if notice
    .col-sm-12
      .alert.alert-success= notice
